<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/managerroot"
	android:background="@color/black" android:layout_width="match_parent"
	android:layout_height="match_parent">
	<TextView android:id="@+id/titlebar" android:layout_width="match_parent"
		android:layout_height="wrap_content" android:textColor="@color/white"
		android:gravity="center_vertical" android:textSize="20sp"
		android:padding="5sp" android:lines="1" android:text="gojappe: FEED MANAGER" />
	<View android:id="@+id/Separator" android:layout_below="@id/titlebar"
		android:layout_width="match_parent" android:background="@color/orange"
		android:layout_height="3dp" />
	<Button android:id="@+id/backb" android:text="BACK"
		android:layout_alignParentBottom="true" android:textSize="15sp"
		android:padding="2sp" android:layout_width="match_parent"
		android:layout_height="wrap_content" />
	<TextView android:id="@android:id/empty" android:layout_below="@id/Separator"
		android:layout_width="match_parent" android:layout_height="match_parent"
		android:background="@color/gray" android:gravity="center"
		android:text="@string/bkmkfoot" />
	<ListView android:id="@android:id/list" android:layout_above="@id/backb"
		android:layout_below="@id/Separator" android:layout_width="match_parent"
		android:layout_height="match_parent" android:divider="@android:drawable/divider_horizontal_dim_dark"
		android:dividerHeight="2dip" />
</RelativeLayout>